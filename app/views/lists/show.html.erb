<h1>All movies in this list</h1>
<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>Comment: <p><%= bookmark.comment %> </p><%= link_to "x", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
    <p>Movie: <%= bookmark.movie.title %></p></li>
  <% end %>

</ul>
